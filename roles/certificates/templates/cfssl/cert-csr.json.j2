  "CN": "{{ out_item.item.cn }}",
  "key": {
    "algo": "{{ cert_key_algo }}",
    "size": {{ cert_key_size }}
  },
  "names": [
    {
      "C": "{{ cert_info_country }}",
      "ST": "{{ cert_info_state }}",
